sectionB:Button("ENHANCED TRUCKING (F)", function()
    local number = math.random(3, 4)
        local player = game.Players.LocalPlayer
        local mouse = player:GetMouse()
        
        local fPressed = false -- keep track of whether F is already pressed
        
        mouse.KeyDown:Connect(function(zz)
            if zz == "f" then
                if not fPressed then -- check if F is already pressed
                    fPressed = true -- mark F as pressed
                    
                    local args = {
                        [1] = {
                            [1] = "Truck"
                        }
                    }
        
                    game:GetService("Players").LocalPlayer.Character.Jersey.Event:FireServer(unpack(args))
        
                    wait(number)
        
                    local args = {
                        [1] = {
                            [1] = "StopTruck"
                        }
                    }
        
                    game:GetService("Players").LocalPlayer.Character.Jersey.Event:FireServer(unpack(args))
                    
                    fPressed = false -- mark F as released
                end
            end
        end)
end)

