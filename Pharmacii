sectionI:Label("HSFL")
sectionI:Button("RANDOM THROW", function()
    local function undetected() -- to everybody using my script ill explain what this does. Its a local function which does nothing were gonna intercept/hook it with a built-in function print l

    end
    
    local hook = hookfunction(print, undetected)
    
    
    
    local playerWithFootball
    for _, player in ipairs(game.Players:GetPlayers()) do
        local character = player.Character
        if character and character:FindFirstChild("Football") then
            playerWithFootball = player
            break
        end
    end
    
    local targetPlayer
    if playerWithFootball then
        local team = playerWithFootball.Team
        for _, player in ipairs(game.Players:GetPlayers()) do
            if player.Team == team and player ~= playerWithFootball then
                targetPlayer = player
                break
            end
        end
    end
    
    if playerWithFootball and targetPlayer then
        local targetRootPart = targetPlayer.Character.HumanoidRootPart
    
        local args = {
            [1] = {
                [1] = "Clicked",
                [2] = playerWithFootball.Character.HumanoidRootPart.Position,
                [3] = targetRootPart.Position + Vector3.new(0, 10, 0),
                [4] = 100,
                [5] = 6
            }
        }
    
        playerWithFootball.Character.Football.Handle.RemoteEvent:FireServer(unpack(args))
    else
        print("Could not find a player with the Football object and a teammate near them.")
    end
end)

