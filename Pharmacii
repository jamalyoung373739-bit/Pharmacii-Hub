sectionB:Label("PHYSICS")
sectionB:Button("AUTO UNSTUN", function()
    local player = game.Players.LocalPlayer
    local jumping = false
    
    function onKeyPress(inputObject, gameProcessedEvent)
        if inputObject.KeyCode == Enum.KeyCode.E and not jumping then 
            local hasFootballTool = player.Character:FindFirstChild("Football") ~= nil
            if not hasFootballTool then
                jumping = true
                wait(0.7)
                player.Character:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
                jumping = false
            end
        end
    end
    game:GetService("UserInputService").InputBegan:Connect(onKeyPress)
end)

