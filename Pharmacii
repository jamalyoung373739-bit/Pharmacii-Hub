    a.Visible = true
    b.Visible = true
            else
                local a = game:GetService("Players").LocalPlayer.PlayerGui.LEAGUE.Chains
    local b = game:GetService("Players").LocalPlayer.PlayerGui.LEAGUE.Control
    
    a.Visible = false
    b.Visible = false
    end
end)

