sectionG:Label('GENERAL')
sectionG:TextField("Speed", "Enter Speed", function(Speed)
    local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
    local HumanoidMT = getrawmetatable(Humanoid)
    setreadonly(HumanoidMT, false)
    
    local OldIndex = HumanoidMT.__index
    HumanoidMT.__index = function(Self, Key)
        if Key == "WalkSpeed" then
            return 17.5
        end
        return OldIndex(Self, Key)
    end
    
    setreadonly(HumanoidMT, true)
    
    
    local function change()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Speed
     end
     
     change()
     
end)
sectionG:TextField("Jump Power", "Enter Power", function(Jump)
    local Humanoid = game:GetService("Players").LocalPlayer.Character.Humanoid
    local HumanoidMT = getrawmetatable(Humanoid)
    setreadonly(HumanoidMT, false)
    
    local OldIndex = HumanoidMT.__index
    HumanoidMT.__index = function(Self, Key)
        if Key == "JumpPower" then
            return 50
        end
        return OldIndex(Self, Key)
    end
    
    print(Humanoid.JumpPower)
setreadonly(HumanoidMT, true)

local function change()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = Jump
game.Players.LocalPlayer.Character.Humanoid:GetPropertyChangedSignal("JumpPower"):Disconnect()
end

change()
end)

sectionG:Label('PHYSICS')
sectionG:Switch("ANTI KNOCKBACK", false, function(antiknock)
    local a = game.Players.LocalPlayer.Character
    if antiknock then
    a["Left Arm"].Size = Vector3.new(20,2,2)
    a["Right Arm"].Size = Vector3.new(20,2,2)
    else
        a["Left Arm"].Size = Vector3.new(1,2,1)
    a["Right Arm"].Size = Vector3.new(1,2,1)
    end
    
end)

sectionG:Button("QB Follow (R)", function()
    local TOOL_NAME = "Football"
        local FOLLOW_DISTANCE = 1
        
        local character = game:GetService("Players").LocalPlayer.Character
        
        
        local isFollowing = false
        
        local function followPlayer()
            for _, player in pairs(game:GetService("Players"):GetPlayers()) do
                local playerCharacter = player.Character
                if playerCharacter and playerCharacter:FindFirstChild(TOOL_NAME) then
                    local torso = playerCharacter:FindFirstChild("HumanoidRootPart") or playerCharacter:FindFirstChild("Torso")
                    if torso then
                        local humanoid = character:WaitForChild("Humanoid")
                        humanoid:MoveTo(torso.Position + torso.CFrame.lookVector * -FOLLOW_DISTANCE, torso)
                    end
                end
            end
        end
        
        local function toggleFollowing()
            isFollowing = not isFollowing
        end
        
        game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
            if not gameProcessed and input.KeyCode == Enum.KeyCode.R then
                toggleFollowing()
            end
        end)
        
        game:GetService("RunService").RenderStepped:Connect(function()
            if isFollowing then
                followPlayer()
            end
        end)
        
end)

sectionG:Button("BIG HEAD ALL", function()
    for i,v in pairs(game.Players:GetPlayers()) do wait()
        if v.Character then
            v.Character.Head.Size = Vector3.new(4,4,4)
            v.Character.Head.Transparency = 1
        end
    end
end)

